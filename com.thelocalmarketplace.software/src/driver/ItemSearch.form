<?xml version="1.0" encoding="UTF-8"?>
<!-- XML document defining a form using the IntelliJ IDEA UI Designer format -->

<!-- Root element: 'form' -->
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="driver.ItemSearch">
  
  <!-- Grid layout with one row and one column -->
  <grid id="27dc6" row-count="1" column-count="1" layout-manager="GridLayoutManager">
    
    <!-- Setting margins to 0 for the grid -->
    <margin top="0" left="0" bottom="0" right="0"/>
    
    <!-- Constraints for the grid layout -->
    <constraints>
      <!-- Position and size of the grid -->
      <xy x="20" y="20" width="500" height="400"/>
      
      <!-- Grid cell constraints for the only cell in the layout -->
      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
    </constraints>
    
    <!-- No additional properties for the grid -->
    <properties/>
    
    <!-- No border for the grid -->
    <border type="none"/>
    
    <!-- No child elements within the grid -->
    <children/>
  </grid>
</form>
