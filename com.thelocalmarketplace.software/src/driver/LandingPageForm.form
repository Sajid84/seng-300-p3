<?xml version="1.0" encoding="UTF-8"?>
<!-- XML document defining a form using the IntelliJ IDEA UI Designer format -->

<!-- Root element: 'form' -->
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="driver.LandingPage">
  
  <!-- Grid layout with GridBagLayout manager -->
  <grid id="27dc6" binding="root" layout-manager="GridBagLayout">
    
    <!-- Constraints for the grid layout -->
    <constraints>
      <!-- Position and size of the grid -->
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    
    <!-- No additional properties for the grid -->
    <properties/>
    
    <!-- No border for the grid -->
    <border type="none"/>
    
    <!-- Child elements within the grid -->
    <children>
      
      <!-- JButton component for starting a session -->
      <component id="bb0d" class="javax.swing.JButton" binding="startSessionButton" default-binding="true">
        
        <!-- Constraints for the JButton component -->
        <constraints>
          <!-- Position and size of the JButton -->
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        
        <!-- Properties for the JButton -->
        <properties>
          <text value="Start Session"/>
        </properties>
      </component>
      
      <!-- Horizontal spacer -->
      <hspacer id="c2a43">
        <!-- Constraints for the horizontal spacer -->
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
      </hspacer>
      
      <!-- Vertical spacer -->
      <vspacer id="c68a9">
        <!-- Constraints for the vertical spacer -->
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
      </vspacer>
      
      <!-- JLabel component for the welcome message -->
      <component id="cd569" class="javax.swing.JLabel" binding="welcomeLabel">
        
        <!-- Constraints for the JLabel component -->
        <constraints>
          <!-- Position and size of the JLabel -->
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        
        <!-- Properties for the JLabel -->
        <properties>
          <text value="Welcome to The Company!"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
