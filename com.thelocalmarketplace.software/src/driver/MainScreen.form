<?xml version="1.0" encoding="UTF-8"?>
<!-- XML document defining a form using the IntelliJ IDEA UI Designer format -->

<!-- Root element: 'form' -->
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="driver.MainScreenForm">
  
  <!-- Grid layout with CardLayout manager -->
  <grid id="27dc6" binding="root" layout-manager="CardLayout" hgap="0" vgap="0">
    
    <!-- Constraints for the grid layout -->
    <constraints>
      <!-- Position and size of the grid -->
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    
    <!-- No additional properties for the grid -->
    <properties/>
    
    <!-- No border for the grid -->
    <border type="none"/>
    
    <!-- No child elements within the grid (CardLayout usually switches between components dynamically) -->
    <children/>
  </grid>
</form>
